{
	"name": "hapify-cli",
	"version": "0.4.1",
	"description": "Command Line Interface for Hapify Cloud",
	"main": "dist/index.js",
	"bin": {
		"hpf": "bin/index.js"
	},
	"preferGlobal": true,
	"scripts": {
		"start": "node lib/index.js",
		"watch": "rm -rf lib && tsc -w",
		"build": "rm -rf lib dist && tsc -p . && grunt",
		"lint": "tslint --fix -p .",
		"test": "echo \"Error: no test specified\" && exit 1",
		"prettier": "./node_modules/.bin/prettier --write \"**/*{.ts,.json,.css,.scss,.html}\""
	},
	"keywords": [
		"hapify",
		"cli"
	],
	"private": false,
	"author": "Tractr <hapify@tractr.net>",
	"license": "UNLICENSED",
	"homepage": "https://www.hapify.io",
	"dependencies": {
