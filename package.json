		"case": "^1.6.1",
		"chalk": "^2.4.1",
		"commander": "^2.17.1",
		"detect-port": "^1.2.3",
		"error-stack-parser": "^2.0.2",
		"hapi": "^17.5.3",
		"hoek": "^6.1.2",
		"inert": "^5.1.2",
		"inquirer": "^6.2.2",
		"joi": "^13.6.0",
		"jsonwebtoken": "^8.3.0",
		"jszip": "^3.1.5",
		"md5": "^2.2.1",
		"mkdirp": "^0.5.1",
		"opn": "^5.4.0",
		"querystring": "^0.2.0",
