{
	"name": "hapify-cli",
	"version": "0.5.4",
	"description": "Command Line Interface for Hapify Cloud",
	"main": "dist/index.js",
	"bin": {
		"hpf": "bin/index.js"
	},
	"preferGlobal": true,
	"scripts": {
		"start": "node lib/index.js",
		"watch": "rm -rf lib && tsc -w",
