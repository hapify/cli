{
  "name": "hapify-cli",
  "version": "0.7.2",
  "description": "Command Line Interface for Hapify",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "hpf": "dist/index.js"
  },
  "preferGlobal": true,
  "scripts": {
    "start": "ts-node src/index.ts",
    "update": "npm-check -u",
    "test": "nyc mocha",
    "mocha": "mocha",
    "lint": "prettier --write '**/*.ts'",
    "build": "rimraf dist && tsc -p . && cp -r node_modules/hapify-gui/dist/hapify-gui dist/html"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com:hapify/cli.git"
  },
  "keywords": [
    "hapify",
    "cli"
  ],
  "author": "Edouard Demotes-Mainard <edouard@tractr.net>",
  "license": "MIT",
  "homepage": "https://www.hapify.io",
  "dependencies": {
    "@hapi/hapi": "^20.0.1",
    "@hapi/hoek": "^9.1.0",
    "@hapi/inert": "^6.0.3",
    "axios": "^0.21.0",
    "case": "^1.6.3",
    "chalk": "^4.1.0",
    "commander": "^6.2.0",
    "detect-port": "^1.2.3",
    "hapify-generator": "github:hapify/generator#v0.5.3",
    "hapify-vm": "github:hapify/vm#v0.3.2",
    "inquirer": "^7.3.3",
    "joi": "^17.3.0",
    "jsonwebtoken": "^8.5.1",
    "jszip": "^3.5.0",
    "md5": "^2.3.0",
    "mkdirp": "^1.0.4",
    "open": "^7.3.0",
    "pkg-dir": "^5.0.0",
    "querystring": "^0.2.0",
    "randomstring": "^1.1.5",
    "reflect-metadata": "^0.1.12",
    "rimraf": "^3.0.2",
    "simple-git": "^2.21.0",
    "typedi": "^0.8.0",
    "vm2": "^3.9.2",
    "ws": "^7.3.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^11.0.0",
    "@commitlint/config-conventional": "^11.0.0",
    "@hapi/code": "^8.0.2",
    "@types/detect-port": "^1.3.0",
    "@types/hapi__hapi": "^20.0.2",
    "@types/hapi__inert": "^5.2.1",
    "@types/http-server": "^0.10.1",
    "@types/inquirer": "^7.3.1",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/md5": "^2.2.1",
    "@types/mkdirp": "^1.0.1",
    "@types/mocha": "^8.0.3",
    "@types/node": "^14.14.6",
    "@types/randomstring": "^1.1.6",
    "@types/rimraf": "^3.0.0",
    "@types/ws": "^7.2.9",
    "hapify-gui": "github:hapify/gui#v0.5.0",
    "husky": "^4.3.0",
    "mocha": "^8.2.0",
    "npm-check": "^5.9.2",
    "nyc": "^15.0.1",
    "prettier": "^2.1.2",
    "pretty-quick": "^3.1.0",
    "ts-node": "^9.0.0",
    "typescript": "^4.0.5"
  },
  "engines": {
    "node": ">=9"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "pretty-quick --staged --pattern \"**/*.ts\""
    }
  }
}
