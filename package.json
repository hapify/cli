		"grunt-contrib-uglify-es": "github:gruntjs/grunt-contrib-uglify#harmony",
		"husky": "^1.3.1",
		"prettier": "^1.15.1",
		"pretty-quick": "^1.8.0",
		"reflect-metadata": "^0.1.12",
		"tslint": "^5.11.0",
		"typedi": "^0.8.0",
