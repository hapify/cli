		"test": "echo \"Error: no test specified\" && exit 1",
		"prettier": "./node_modules/.bin/prettier --write \"**/*{.ts,.json,.css,.scss,.html}\""
	},
	"keywords": [
		"hapify",
		"cli"
	],
	"private": false,
	"author": "Tractr <hapify@tractr.net>",
	"license": "LicenseRef-LICENSE",
	"homepage": "https://www.hapify.io",
