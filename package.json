		"@types/detect-port": "^1.1.0",
		"@types/error-stack-parser": "^2.0.0",
		"@types/hapi": "^17.0.18",
		"@types/hoek": "^4.1.3",
		"@types/http-server": "^0.10.0",
		"@types/inquirer": "0.0.43",
		"@types/joi": "^13.6.0",
		"@types/jsonwebtoken": "^7.2.8",
