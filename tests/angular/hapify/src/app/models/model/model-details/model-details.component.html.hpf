<div class="container">
    <div class="row">
        <div class="col-12 my-3 my-lg-5">
            <mat-card>
<<@ F /ip/pr f>>
                <!-- <<f A>> -->
                <div class="mb-4">
                    <h5>{{ '<<M a_a>>_title_<<f a_a>>' | translate }}</h5>
    <<? f tD>>
                    <div>{{ <<M aA>>.props.<<f a_a>> | date:'medium' }}</div>
    <<?? f tE>>
                    <div *ngIf="<<M aA>>.<<f aA>>Exists()">
        <<? f ml>>
                        <div *ngFor="let item of <<M aA>>.props.<<f a_a>>">
                            <a class="mr-2" [routerLink]="['/<<f.m a-a>>', item.getId()]">
                                {{ item.getLabel() }}
                            </a>
                        </div>
        <<??>>
                        <a [routerLink]="['/<<f.m a-a>>', <<M aA>>.props.<<f a_a>>.getId()]">
                            {{ <<M aA>>.props.<<f a_a>>.getLabel() }}
                        </a>
        <<?>>
                    </div>
    <<?? f tB>>
                    <div>
                        <span *ngIf="<<M aA>>.props.<<f a_a>> === true || <<M aA>>.props.<<f a_a>> === false">
                            {{ (<<M aA>>.props.<<f a_a>> ? '<<M a_a>>_<<f a_a>>_true' : '<<M a_a>>_<<f a_a>>_false') | translate }}
                        </span>
                    </div>
    <<?? f tSt>>
                    <div [innerHTML]="<<M aA>>.props.<<f a_a>> | nl2br"></div>
    <<?? f tSr>>
                    <mat-card [innerHTML]="<<M aA>>.props.<<f a_a>>"></mat-card>
    <<??>>
                    <div>{{ <<M aA>>.props.<<f a_a>> }}</div>
    <<?>>
                </div>
                <!-- /<<f A>> -->
<<@>>
                <mat-divider class="mb-4" [inset]="true"></mat-divider>
                <div class="row">
                    <div class="col-6">
                        <a [routerLink]="['/<<M a-a>>/edit', <<M aA>>.getId()]" mat-raised-button color="primary">
                            {{ '<<M a_a>>_action_edit' | translate }}
                        </a>
                        <a [routerLink]="['/<<M a-a>>/create']" mat-raised-button color="secondary" class="ml-2">
                            {{ '<<M a_a>>_action_create' | translate }}
                        </a>
                    </div>
                    <div class="col-6" *ngIf="deletable">
                        <div class="float-right">
                            <mat-slide-toggle color="warn"
                                              [checked]="deletionUnlocked"
                                              (change)="onDeletionUnlock($event)"
                            ></mat-slide-toggle>
                            <button class="ml-2"
                                    [disabled]="!deletionUnlocked"
                                    (click)="onDeleteClick()"
                                    mat-raised-button
                                    color="warn">
                                {{ '<<M a_a>>_action_delete' | translate }}
                            </button>
                        </div>
                    </div>
                </div>
            </mat-card>
        </div>
    </div>
</div>
