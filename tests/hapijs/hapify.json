{
  "validatorPath": "hapify/validator.js",
  "modelsPath": "../models.json",
  "templates": [
    {
      "name": "Create",
      "path": "routes/{model.hyphen}/create.js",
      "engine": "hpf",
      "input": "one",
      "contentPath": "hapify/src/routes/model/create.js.hpf"
    },
    {
      "name": "Read",
      "path": "routes/{model.hyphen}/read.js",
      "engine": "hpf",
      "input": "one",
      "contentPath": "hapify/src/routes/model/read.js.hpf"
    },
    {
      "name": "Update",
      "path": "routes/{model.hyphen}/update.js",
      "engine": "hpf",
      "input": "one",
      "contentPath": "hapify/src/routes/model/update.js.hpf"
    },
    {
      "name": "Delete",
      "path": "routes/{model.hyphen}/delete.js",
      "engine": "hpf",
      "input": "one",
      "contentPath": "hapify/src/routes/model/delete.js.hpf"
    },
    {
      "name": "List",
      "path": "routes/{model.hyphen}/list.js",
      "engine": "hpf",
      "input": "one",
      "contentPath": "hapify/src/routes/model/list.js.hpf"
    },
    {
      "name": "Count",
      "path": "routes/{model.hyphen}/count.js",
      "engine": "hpf",
      "input": "one",
      "contentPath": "hapify/src/routes/model/count.js.hpf"
    },
    {
      "name": "Includes",
      "path": "routes/{model.hyphen}/inc.js",
      "engine": "hpf",
      "input": "one",
      "contentPath": "hapify/src/routes/model/inc.js.hpf"
    },
    {
      "name": "Index",
      "path": "routes/{model.hyphen}/index.js",
      "engine": "hpf",
      "input": "one",
      "contentPath": "hapify/src/routes/model/index.js.hpf"
    },
    {
      "name": "Index All",
      "path": "routes/index.js",
      "engine": "hpf",
      "input": "all",
      "contentPath": "hapify/src/routes/index.js.hpf"
    }
  ]
}
