{
  "name": "HapiJS",
  "description": "NodeJS Backend with MongoDB",
  "logo": "https://hapijs.com/public/img/logo.svg",
  "project": "5c38bdc2917d3a00100ae7da",
  "validatorPath": "hapify/validator.js",
  "templates": [
    {
      "name": "Count",
      "path": "src/routes/{model.hyphen}/count.js",
      "engine": "hpf",
      "input": "one"
    },
    {
      "name": "Create",
      "path": "src/routes/{model.hyphen}/create.js",
      "engine": "hpf",
      "input": "one"
    },
    {
      "name": "Delete",
      "path": "src/routes/{model.hyphen}/delete.js",
      "engine": "hpf",
      "input": "one"
    },
    {
      "name": "Includes",
      "path": "src/routes/{model.hyphen}/inc.js",
      "engine": "hpf",
      "input": "one"
    },
    {
      "name": "Index",
      "path": "src/routes/{model.hyphen}/index.js",
      "engine": "hpf",
      "input": "one"
    },
    {
      "name": "Index All",
      "path": "src/routes/index.js",
      "engine": "hpf",
      "input": "all"
    },
    {
      "name": "List",
      "path": "src/routes/{model.hyphen}/list.js",
      "engine": "hpf",
      "input": "one"
    },
    {
      "name": "Read",
      "path": "src/routes/{model.hyphen}/read.js",
      "engine": "hpf",
      "input": "one"
    },
    {
      "name": "Update",
      "path": "src/routes/{model.hyphen}/update.js",
      "engine": "hpf",
      "input": "one"
    }
  ]
}
