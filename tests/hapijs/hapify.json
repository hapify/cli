{
  "name": "HapiJS",
  "description": "NodeJS Backend with MongoDB",
  "logo": "https://hapijs.com/public/img/logo.svg",
  "project": "5c38bdc2917d3a00100ae7da",
  "validatorPath": "hapify/validator.js",
  "defaultFields": [
    {
      "name": "_id",
      "type": "string",
      "subtype": null,
      "reference": null,
      "primary": true,
      "unique": false,
      "label": false,
      "nullable": false,
      "multiple": false,
      "important": false,
      "searchable": false,
      "sortable": false,
      "isPrivate": false,
      "internal": true,
      "restricted": false,
      "ownership": true
    },
    {
      "name": "created_at",
      "type": "datetime",
      "subtype": null,
      "reference": null,
      "primary": false,
      "unique": false,
      "label": false,
      "nullable": false,
      "multiple": false,
      "important": false,
      "searchable": false,
      "sortable": true,
      "isPrivate": false,
      "internal": true,
      "restricted": false,
      "ownership": false
    }
  ],
  "templates": [
    {
      "path": "src/routes/{model.hyphen}/count.js",
      "engine": "hpf",
      "input": "one"
    },
    {
      "path": "src/routes/{model.hyphen}/create.js",
      "engine": "hpf",
      "input": "one"
    },
    {
      "path": "src/routes/{model.hyphen}/delete.js",
      "engine": "hpf",
      "input": "one"
    },
    {
      "path": "src/routes/{model.hyphen}/inc.js",
      "engine": "hpf",
      "input": "one"
    },
    {
      "path": "src/routes/{model.hyphen}/index.js",
      "engine": "hpf",
      "input": "one"
    },
    {
      "path": "src/routes/{model.hyphen}/list.js",
      "engine": "hpf",
      "input": "one"
    },
    {
      "path": "src/routes/{model.hyphen}/read.js",
      "engine": "hpf",
      "input": "one"
    },
    {
      "path": "src/routes/{model.hyphen}/update.js",
      "engine": "hpf",
      "input": "one"
    },
    {
      "path": "src/routes/index.js",
      "engine": "hpf",
      "input": "all"
    }
  ]
}